namespace SeleDreams.DataManagement.BattleData
{
    using SeleDreams.DataManagement.Interfaces;
    public class Character : INamed, IIdentifiable, IHasProgrammingType
    {
        protected string _name;
        public string Name
        {
            get => _name;
            set => _name = value;
        }

        protected ProgrammingType _ptype;
        public ProgrammingType PType
        {
            get => _ptype;
            set => _ptype = value;
        }

        protected int _id;
        public int ID
        {
            get => _id;
            set => _id = value;
        }

    }
}
